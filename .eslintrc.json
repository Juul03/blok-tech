{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-duplicate-imports": ["warn", "always"], 
        "no-use-before-define": ["error", "always"],
        "arrow-body-style": ["error", "always"],
        "camelcase": ["warn", "always"],
        "curly":["warn", "all"],
        "func-name-matching": ["warn", "allways"],
        "no-empty-function": ["warn", "always"],
        "no-var":["warn", "allways"],
        "prefer-const": ["warn", "all"],
        "vars-on-top": ["warn", "all"],
        "array-bracket-spacing": ["warn", "all"],
        "arrow-spacing": ["warn", "all"],
        "comma-spacing": ["warn", "all"],
        "max-len": ["warn", "all"],
        "no-multi-spaces": ["warn", "all"],
        "no-trailing-spaces": ["warn", "all"],
        "quotes":["error", "single"]
    }
}
